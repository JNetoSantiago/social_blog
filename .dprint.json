{
    "lineWidth": 120,
    "typescript": {
      "indentWidth": 2,
      "semiColons": "asi",
      "quoteStyle": "preferSingle",
      "binaryExpression.operatorPosition": "sameLine",
      "jsx.quoteStyle": "preferDouble",
      "taggedTemplate.spaceBeforeLiteral": false,
      "trailingCommas": "never"
    },
    "json": { "indentWidth": 2 },
    "markdown": {},
    "toml": {},
    "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml}"],
    "excludes": ["**/node_modules", "**/*-lock.json", "**/public/build", "**/functions"],
    "plugins": [
      "https://plugins.dprint.dev/typescript-0.71.1.wasm",
      "https://plugins.dprint.dev/json-0.15.3.wasm",
      "https://plugins.dprint.dev/markdown-0.13.3.wasm",
      "https://plugins.dprint.dev/toml-0.5.4.wasm"
    ]
}